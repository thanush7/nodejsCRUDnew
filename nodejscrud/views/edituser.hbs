<h3>Edit  Student</h3>

{{#if msg}}
<div class="alert">{{msg}}</div>
{{/if}}


{{#each rows}}
<form method="post" action="/edituser/{{this.id}}" autocomplete="false">
<div class="form-group">
    <label for="name">Name</label>
    <input type="text" name="name" value="{{this.nam}}">
</div>
<div class="form-group">
    <label for="Age">Age</label>
    <input type="text" name="age" value="{{this.age}}">
</div>
<div class="form-group">
    <label for="City">City</label>
    <input type="text" name="city" value="{{this.city}}">
</div>
<div class="form-group">
    <button type="submit">Save</button>
</div>
</form>
{{/each}}